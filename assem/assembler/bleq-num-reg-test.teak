# converts a number to a string and prints it to the screen
.screen_init: tale_of_two_cities.txt # 128x64 = 8192

.glyph_init: ascii_8x8.png # 256x4 = 1024

.teak # beginning of assembly code

      # blt $r1, $r2 # $r1 < $r2
      # bgeq $r1, $r2 # blt $r2, $r1 # $r1 >= $r2

      # bleq $r1, $r2 # $r1 <= $r2
      # bgt $r1, $r2 # bleq $r2, $r1 # $r1 > $r2

      li $1, 100
      li $2, 100 # check equals
      li $3, 80 # check less than

      bleq $1, $2, eq
      li $4, 0 # equal check fail

      bleq $1, $3, less
      li $5, 0 # less than fail
      jl fin

eq:   li $4, 1 # equal check pass

      bleq $1, $3, less
      li $5, 0 # less than fail
      jl fin

less: li $5, 1 # less than pass

fin:  jl fin
